module imageRom #(parameter startAddress=0,parameter endAddress=255)(input [10:0] address,output [31:0] Q);
	logic [31:0] memory [endAddress:startAddress];
	initial $readmemh("Processor.mem",memory); //aqui hay que cambiar el archivo para el de la imagen que mando el profe de imagen
	
	

	
	assign Q=memory[address];

endmodule 